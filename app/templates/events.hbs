<p>Let's all get together! Create a {{link-to "New Event" "new_event" class='btn btn-primary header-btn'}} right here.</p>

<section>
  {{#each}}
    <article>
      <div class='wrapper'>
      <h2 class='entry-title'>{{eventTitle}}</h2>
      <h3 class='text-output'>Event Date: {{eventDate}}</h3>
      <h3 class='text-output'>Event Time: {{eventTime}}</h3>
      <h3 class='text-output'>{{eventPost}}</h3>
      </div>
      <h3>{{link-to 'Edit' 'edit_event' this class='edit-link' classNames='btn btn-primary'}}</h3>
    </article>
  {{/each}}
</section>
